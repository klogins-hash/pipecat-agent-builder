# Pipecat Agent Builder - Environment Variables for Pipecat Cloud

# Required AI Service API Keys
DEEPGRAM_API_KEY=your_deepgram_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
CARTESIA_API_KEY=your_cartesia_api_key_here

# Optional: Additional AI Services
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Agent Generation Settings
AGENT_BUILDER_NAME="Pipecat Agent Builder"
AGENT_BUILDER_PERSONALITY="friendly_professional"
DEFAULT_VOICE_ID="71a7ad14-091c-4e8e-a314-022ece01c121"

# Logging Level
LOG_LEVEL=INFO

# Optional: Email delivery for generated agents
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
FROM_EMAIL=builder@pipecat.ai
